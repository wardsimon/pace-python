pip install cmake
mkdir mcrbuild
mkdir mcr
cd mcrbuild
wget -q  https://ssd.mathworks.com/supportfiles/downloads/R2020b/Release/6/deployment_files/installer/complete/glnxa64/MATLAB_Runtime_R2020b_Update_6_glnxa64.zip
unzip -q MATLAB_Runtime_R2020b_Update_6_glnxa64.zip
rm -f MATLAB_Runtime_R2020b_Update_6_glnxa64.zip
./install -destinationFolder ~/mcr -agreeToLicense yes -mode silent
cd ../
rm -rf mcrbuild
 
export Matlab_ROOT_DIR=~/mcr/
python38 setup.py install
